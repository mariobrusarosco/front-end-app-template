#!/usr/bin/env node

const path = require('path')
const tsConfig = path.join(__dirname, '../tsconfig.json')
const devMode = process.env.DEV_MODE



if(devMode) {
  console.log("Starting Front End Template - Development mode")
  require('ts-node').register(tsConfig)
  require('../src')
} else {

  console.log("Starting Front End Template")
  require('../lib')
}




