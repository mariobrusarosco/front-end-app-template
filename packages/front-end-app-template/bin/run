#!/usr/bin/env node

const path = require('path')
const tsConfig = path.join(__dirname, '../tsconfig.json')
const isDevMode = process.env.DEV_MODE == "true"


if(isDevMode) {
  console.log("Starting Front End Template - Development mode")

  require('ts-node').register({ project: tsConfig })
  require('../src').default.argv

} else {
  console.log("Starting Front End Template")

  return require('../lib').default.argv
}
